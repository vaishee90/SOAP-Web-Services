        contract="StorageService.IStorageService" name="BasicHttpBinding_IStorageService" />
      <endpoint address="http://localhost:3726/TopicalNewsService.svc"
        binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_ITopicalNewsService"
